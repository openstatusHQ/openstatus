TAGS "dns"

NODE endpoint
SQL >

    %
    SELECT
        time as day,
        monitorId,
        countMerge(count) as count,
        countMerge(success) as ok,
        countMerge(error) as error,
        countMerge(degraded) as degraded
    FROM mv__dns_status_45d__v0
    WHERE monitorId IN {{ Array(monitorIds, 'String', '7417') }}
    GROUP BY day, monitorId
    ORDER BY day DESC


